{
  "name": "mkit-bundle-gatsby",
  "version": "1.0.0",
  "description": "Micro Gatsby project bundle",
  "keywords": [
    "mkit",
    "bundle",
    "micro",
    "Gatsby",
    "web",
    "project",
    "boilerplate",
    "template",
    "starter"
  ],
  "scripts": {
    "lint": "eslint --fix src/**/*.js && prettier --write src/**/*.{js,json,md}",
    "develop": "gatsby develop",
    "build:dir": "mkdir -p public/icons/",
    "build:favicons": "node scripts/favicons",
    "build:gatsby": "gatsby build",
    "build": "npm run build:dir && npm run build:favicons && npm run build:gatsby",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "favicons": "^5.1.1",
    "gatsby": "^2.0.19",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-manifest": "^2.0.4",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-webpack-size": "0.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  },
  "author": "MK IT Ltd. <hi@mkit.io>",
  "homepage": "https://github.com/mkitio/mkit-bundle-gatsby#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkitio/mkit-bundle-gatsby.git"
  },
  "bugs": {
    "url": "https://github.com/mkitio/mkit-bundle-gatsby/issues"
  },
  "license": "MIT"
}
